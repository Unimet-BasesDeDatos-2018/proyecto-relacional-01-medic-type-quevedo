<div class="banner">
</div>
<div class="container">
  <div class="row">
    <div class="col m12">
      <h6 class="center">Historia Médica de  <%= paciente.Nombre+' '+paciente.Apellido%></h6>
    </div>
  </div>
  <div class="row card-panel">
      <div class="col m12">
        <h3 class="center">Lista de Consultas</h3>
      </div>
      
      <div class="col m12">
        <ul class="collapsible" data-collapsible="accordion">
          <% consultas.forEach(function (consulta) { %>
          <li>
            <div class="collapsible-header"><i class="material-icons">filter_drama</i>
              <div class="col s1"><%= consulta.Fecha %></div>
              <div class="col s1"><%= consulta.Tipo %></div>
            </div>
            <div class="collapsible-body"><span>Pérate menol.</span></div>
          </li>
          <%})%>
        </ul>
      </div>
    </div>

  <div class="row">
    <div class="col m12">
      
    </div>
  </div>
</div>
