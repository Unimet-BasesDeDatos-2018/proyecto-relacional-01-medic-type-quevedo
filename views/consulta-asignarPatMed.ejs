

<div class="row card-panel">
        <div class="col m12">
          <div class="row">
            <div class="col m12">
              <h5 class="center">Asignar Enfermedades y Medicamentos </h5>
            </div>
          </div>
          <div class="row">
            <div class="col m12">
              <div class="row">
                <div class="col m6 input-field">
                   
                    <div class="input-field col s12">
                      
                        <select multiple>
                          <option value="" disabled selected>Medicamentos</option>
                          
                  
                        <% medicamentos.forEach(function (medicamento) { %>
                            <option value="<%= medicamento.id %>"> <%= medicamento.Nombre %> </option>
                          <%})%>

                        </select>
                      </div>

                </div>
                <div class="col m6 input-field">
                    <div class="input-field col s12">
                        
                          <select multiple>
                            <option value="" disabled selected>Patologias</option>
                       
                            <% patologias.forEach(function (patologia) { %>
                              <option value="<%= patologia.id %>"> <%= patologia.Nombre %> </option>
                              <%})%>

                            </select>
                          </div>
                      
                </div>
            </div>
              
            </div>
          </div>
    </div>
</div>



</div>
</div>
